{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "ordered-imports": false,
        "member-access": false,
        "no-var-requires": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "variable-name": false,
        "no-empty": false
    }
}