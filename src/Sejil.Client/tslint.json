{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "object-literal-sort-keys": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-console": [
            false
        ],
        "member-access": [
            false
        ],
        "member-ordering": [
            false
        ],
        "max-line-length": [
            150
        ],
        "no-var-requires": false,
        "arrow-parens": false,
        "space-before-function-paren": false
    },
    "rulesDirectory": []
}