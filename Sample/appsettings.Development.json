{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "Using": ["LogsExplorer"],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [{
      "Name": "LogsExplorer",
      "Args": {
        "sqliteDbPath": "logs.db",
        "restrictedToMinimumLevel": "Debug"
      }
    }],
    "Enrich": ["FromLogContext"],
    "Properties": {
      "Application": "Sample"
    }
  }
}